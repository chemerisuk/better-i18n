{
  "name": "better-i18n-plugin",
  "description": "Internationalization plugin for better-dom",
  "version": "2.0.0",
  "author": "Maksim Chemerisuk",
  "license": "MIT",
  "homepage": "https://github.com/chemerisuk/better-i18n-plugin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chemerisuk/better-i18n-plugin"
  },
  "keywords": [
    "ecosystem:better-dom",
    "better-i18n-plugin",
    "i18n",
    "l10n"
  ],
  "bugs": {
    "url": "https://github.com/chemerisuk/better-i18n-plugin/issues"
  },
  "config": {
    "gulp": "--gulpfile node_modules/better-dom-boilerplate/gulpfile.js --cwd ."
  },
  "scripts": {
    "test": "gulp test $npm_package_config_gulp",
    "start": "gulp dev $npm_package_config_gulp",
    "version": "gulp dist $npm_package_config_gulp && git add -A dist bower.json",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "better-dom": ">=2.0.0"
  },
  "devDependencies": {
    "better-dom-boilerplate": "git://github.com/chemerisuk/better-dom-boilerplate.git"
  },
  "karmaFiles": [
    "node_modules/better-dom/dist/better-dom.js"
  ]
}
